{
	"name": "miso-airway-survey",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "BROWSER=brave-dev vite dev --open --host",
		"migrate": "bun scripts/migrate/migrate.ts",
		"migrate-prod": "NODE_ENV=production npm run migrate",
		"migrate-test": "npm run migrate -- -t",
		"check-data": "bun scripts/scrape/check.ts",
		"deploy": "bun scripts/pushEnvToVercel && vercel build --prod && vercel --prebuilt --prod",
		"build": "vite build",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch"
	},
	"engines": {
		"node": ">= 22.15.1"
	},
	"dependencies": {
		"@vercel/analytics": "^1.5.0",
		"bluebird": "^3.7.2",
		"fs-extra": "^11.3.0",
		"glob": "^11.0.2",
		"kit-query-params": "^0.0.26",
		"lodash.merge": "^4.6.2",
		"luxon": "^3.6.1",
		"minimatch-browser": "^1.0.0",
		"mkdirp": "^3.0.1",
		"mysql2": "^3.14.1",
		"slugify": "^1.6.6",
		"string-similarity-js": "^2.1.4",
		"superjson": "^2.2.2",
		"svelte-kit-cookie-session": "^4.1.1",
		"svelte-tiny-query": "^0.1.5",
		"sveltekit-search-params": "^3.0.0"
	},
	"devDependencies": {
		"@iconify/json": "^2.2.336",
		"@sveltejs/adapter-vercel": "^5.7.2",
		"@sveltejs/kit": "^2.21.2",
		"@sveltejs/vite-plugin-svelte": "^5.0.0",
		"bcrypt": "^6.0.0",
		"chokidar": "^4.0.3",
		"mdsvex": "^0.12.6",
		"patch-package": "^8.0.0",
		"sass": "^1.87.0",
		"svelte": "^5.0.0",
		"svelte-check": "^4.0.0",
		"typescript": "^5.0.0",
		"unplugin-icons": "^22.2.0",
		"vite": "^6.2.6",
		"yargs": "^17.7.2"
	}
